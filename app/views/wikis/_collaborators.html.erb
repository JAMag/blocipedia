<h2> Collaborators </h2>

<ul>
<% @collaborators.each do |c| %>
<li>
<%= c.email %>
</li>
<% end %>
</ul>

<%= form_for [wiki, Collaboration.new] do |f| %>
<%= f.select :user_id, options_from_collection_for_select(@users, 'id', 'email') %>
  <div class="form-group">
   <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
<% end %>